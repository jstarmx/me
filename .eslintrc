{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jasmine": true,
    "node": true,
  },
  "rules": {
    "comma-dangle": [0, {
      "arrays": "never",
      "objects": "always-multiline",
      "imports": "never",
      "exports": "never",
      "functions": "never",
    }],
    "indent": 0,
    "no-underscore-dangle": 0,
    "space-before-function-paren": [2, "always"],
    "react/prefer-es6-class": [2, "never"],
  },
}
