{
  "name": "me",
  "version": "0.0.1",
  "description": "My personal website",
  "engines": {
    "node": "7.1.0"
  },
  "scripts": {
    "dev": "rm -rf ./public && webpack -d --watch",
    "postinstall": "rm -rf ./public && webpack -p",
    "lint": "eslint ./ --ext .js --ext .jsx && stylelint ./app/styles/**/*.scss",
    "test": "eslint ./ --ext .js --ext .jsx && stylelint ./app/styles/**/*.scss && jest --coverage && cat ./__coverage__/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "James Starkie",
  "license": "MIT",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/public/**",
      "!**/__coverage__/**",
      "!webpack.config.js"
    ],
    "coverageDirectory": "./__coverage__",
    "setupFiles": [
      "./lib/test-env.js"
    ]
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.25.0",
    "ejs": "^2.5.2",
    "events": "^1.1.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "flux": "^3.1.0",
    "import-glob-loader": "^1.1.0",
    "lodash": "^4.17.2",
    "node-sass": "^3.12.2",
    "postcss-loader": "^1.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.0.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "stylelint-webpack-plugin": "^0.4.0",
    "superagent": "^2.3.0",
    "webpack": "^1.12.9",
    "webpack-merge": "^0.17.0",
    "webpack-notifier": "^1.4.1",
    "webpack-validator": "^2.2.9"
  },
  "devDependencies": {
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "coveralls": "^2.11.15",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "jest": "^17.0.3",
    "jsdom": "^9.8.3",
    "react-addons-test-utils": "^15.4.0",
    "react-test-renderer": "^15.4.0",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0"
  }
}
